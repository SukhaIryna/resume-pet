<main>
    <div class="nav">
        {{#with navigation}}
        {{#each links}}
        <div class="nav-item">
            <a href="{{href}}" class="nav-link {{#if @last}}disabled{{/if}}"> {{text}}</a>

        </div>

        {{/each}}
        {{/with}}
    </div>






    <hr class=" m-4">
    <div class="row col-12">
        {{#each footer}}
        <div class="col-2 p-1">
            {{#each this}}
            <div class="nav">
                <div class="nav item">
                    <a href="{{link}}" class="nav-link {{#if @last}}disabled{{/if}}">{{text}}</a>
                </div>
            </div>
            {{/each}}
        </div>
        {{/each}}
    </div>


    </div>
</main>